<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias alias="MyMovie" type="team_movie.model.MovieBean" />
		<typeAlias alias="MyGenre" type="team_movie.model.GenreBean"/>
		<typeAlias alias="MyUser" type="team_movie.model.UserBean"/>
		<typeAlias alias="MyBoard" type="team_movie.model.BoardBean"/>
		<typeAlias alias="MyMembership" type="team_movie.model.MembershipBean"/>
		<typeAlias alias="MyFavorite" type="team_movie.model.FavoriteBean"/>
		<typeAlias alias="MyLatestview" type="team_movie.model.LatestviewBean"/>
	
	</typeAliases>
	 
	<mappers>
		<mapper resource="team_movie/mybatis/movie.xml" />
		<mapper resource="team_movie/mybatis/genre.xml" />
		<mapper resource="team_movie/mybatis/user.xml" />
		<mapper resource="team_movie/mybatis/board.xml" />
		<mapper resource="team_movie/mybatis/membership.xml" />
		<mapper resource="team_movie/mybatis/favorite.xml" />
		<mapper resource="team_movie/mybatis/latestview.xml" />
	</mappers>

</configuration>


